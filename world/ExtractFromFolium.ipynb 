   "outputs": [],
   "source": [
    "countries = []\n",
    "for item in wc['features']:\n",
    "    c = item['properties']['name']\n",
    "    ID = item['id']\n",
    "    countries.append({\"name\" : c, \"ID\":ID})"
   ]
  },
